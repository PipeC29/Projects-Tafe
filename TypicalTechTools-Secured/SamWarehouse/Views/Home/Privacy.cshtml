@{
    ViewData["Name"] = "About Us";
}
<h1>@ViewData["Name"]</h1>

<main>
    <section>
        <h3>Overview</h3>
        <p>Sam's Warehouse is a food and grocery retailer.</p>
    </section>

    <section>
        <h3>Mission</h3>
        <p>Our mission is to provide our customers with the best products and services at competitive prices. We are committed to providing a convenient and enjoyable shopping experience for all our customers.</p>
    </section>

    <section>
        <h3>Products and Services</h3>
        <ul>
            <li>Fresh produce</li>
            <li>Packaged goods</li>
            <li>Cleaning supplies</li>
            <li>Personal care products</li>
            <li>Home delivery</li>
            <li>In-store pickup</li>
        </ul>
    </section>

    <section>
        <h3>Hours of Operation</h3>
        <p>Monday through Friday: 8:00 AM to 8:00 PM</p>
        <p>Saturdays: 8:00 AM to 7:00 PM</p>
        <p>Sundays: 9:00 AM to 6:00 PM</p>
    </section>

    <section>
        <h3>Contact Information</h3>
        <p>Phone: 123456789</p>
        <p>Email: samwarehouse@email.com</p>
    </section>
</main>

